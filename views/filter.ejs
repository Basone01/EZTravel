<section class="main-content" id="findyourplace">
<form class="full-form" action="/search" method="get">
  <div class="form-block" >
    <div class="head"><h3>Filter</h3></div>
    <div class="form-box">
      <div class="col-1-3">
        <div class="form-block"  id="province">
          <div class="head"><h3>Province</h3></div>
          <div class="headingbox">
            <input type="text" class="province-select" name="province" list="provincelist" placeholder="Provinces">
            <datalist id="provincelist">
              <% provinces.forEach(function(province){%>
                <option value="<%=province%>"><%=province%></option>
              <%});%>
            </datalist>

          </div>
        </div>
        <div class="form-block"  id="Accomodation">
          <div class="head"><h3>Accomodation</h3></div>
          <div class="headingbox">
            <label ><input type="checkbox" name="accomodation" value="hotel"> HOTEL</label>
            <label ><input type="checkbox" name="accomodation" value="mansion"> MANSION</label>
            <label ><input type="checkbox" name="accomodation" value="resort"> RESORT</label>
            <label ><input type="checkbox" name="accomodation" value="house"> HOUSE</label>
          </div>
        </div>
      </div>

      <div class="col-1-3">
        <div class="form-block"  id="price">
          <div class="head"><h3>Price</h3></div>
          <div class="headingbox">
            <input  type="range" id="minprice" class="styled-range" min="200" max="20000" value="200" step="50" style="width:100%;">
            <div class="text-centered" style="display:block;">
              <label class="inline"><input class="range-display rounded-border" id="minprice-display" type="number" name="minprice"
                value="200" step="50" min="200" max="20000" style="width:4rem;text-indent:0.6rem;"> -</label>
              <label class="inline"><input class="range-display rounded-border" id="maxprice-display" type="number" name="maxprice"
                value="20000" step="50" min="200" max="20000" style="width:4rem;text-indent:0.6rem;"> Baht</label>
            </div>
            <input  type="range" id="maxprice" class="styled-range" min="200" max="20000" value="20000" step="50" style="width:100%;">


          </div>
        </div>
        <div class="form-block"  id="grade">
          <div class="head"><h3>Grade</h3></div>
          <div class="headingbox">
            <fieldset class="rating">
              <input type="radio" id="star5-0" name="rating" value="5" /><label class="full" for="star5-0" title="5 stars"></label>
              <input type="radio" id="star4-5" name="rating" value="4.5" /><label class="half" for="star4-5" title="4.5 stars"></label>
              <input type="radio" id="star4-0" name="rating" value="4" /><label class="full" for="star4-0" title="4 stars"></label>
              <input type="radio" id="star3-5" name="rating" value="3.5" /><label class="half" for="star3-5" title="3.5 stars"></label>
              <input type="radio" id="star3-0" name="rating" value="3" /><label class="full" for="star3-0" title="3 stars"></label>
              <input type="radio" id="star2-5" name="rating" value="2.5" /><label class="half" for="star2-5" title="2.5 stars"></label>
              <input type="radio" id="star2-0" name="rating" value="2" /><label class="full" for="star2-0" title="2 stars"></label>
              <input type="radio" id="star1-5" name="rating" value="1.5" /><label class="half" for="star1-5" title="1.5 stars"></label>
              <input type="radio" id="star1-0" name="rating" value="1" /><label class="full" for="star1-0" title="1 star"></label>
              <input type="radio" id="star0-5" name="rating" value="0.5" checked="checked"/><label class="half" for="star0-5" title="0.5 stars"></label>
          </fieldse>
          </div>
        </div>

      </div>

      <div class="col-1-3">
        <div class="form-block"  id="facility">
          <div class="head"><h3>Facility</h3></div>
          <div class="headingbox">
            <label><input type="checkbox" name="facility" value="wifi"> WIFI</label>
            <label><input type="checkbox" name="facility" value="fitness"> Fitness</label>
            <label><input type="checkbox" name="facility" value="pool"> Swimming Pool</label>
            <label><input type="checkbox" name="facility" value="restaurant"> Restaurant</label>
            <label><input type="checkbox" name="facility" value="park"> Park</label>
            <label><input type="checkbox" name="facility" value="club"> Night Club</label>
          </div>
        </div>

      </div>

    </div>
    <div id="form-submit">
      <button type="submit">GO!!</button>
    </div>
  </div>
</form>


<script type="text/javascript" src="styles/rangeslider.js"></script>
